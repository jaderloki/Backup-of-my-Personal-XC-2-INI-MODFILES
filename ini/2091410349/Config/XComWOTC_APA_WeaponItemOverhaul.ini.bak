; ==============================
; ||  Setup & Loot Variables  ||
; ==============================


bSINGLE_BUILD_ITEMS = true								;If true, sets up single build item costs instead of schematics
bAUTODETECT_PROTOTYPE_ARMOURY = true					;If true and Prototype Armoury is active, forces single build item cost setup
bGRANT_TLP_PROTOTYPES_ON_RESEARCH = true				;If true, grants the TLP prototype weapons when research is completed if using single build items
														;Has no effect when using infinite items - Disabled (already included) if using Prototype Armoury.
bBUILDABLE_PROTOTYPES = true							;If Prototypes on Research is true, this controls whether TLP prototype weapons remain buildable or not.
														
bREWORK_WEAPON_COSMETICS = true							;Update weapon default cosmetics to better fit with upgrade options/functions (Recommended)

bREWORK_ARMOR_STATS = true								;If true, changes stats on armor items


